.btns {
  position: absolute;
  bottom: 32rpx;
  left: 0;
  width: 750rpx;
  padding: 32rpx 64rpx;
  align-items: flex-start;
  gap: 32rpx;
  background-color: #151515;
  z-index: 1099;
}
.btns .btn {
  width: 400rpx;
  padding: 32rpx;
  border-radius: 56rpx;
  gap: 32rpx;
  background: #444;
}
.textstyle {
  color: #fff;
  font-size: 36rpx;
  font-weight: 400;
  line-height: 48rpx;
}
.title {
  color: #444;
  font-size: 24rpx;
  font-style: normal;
  font-weight: 600;
  line-height: 32rpx;
}
.mb-60 {
  margin-bottom: 60rpx;
}
.m-50 {
  margin: 50rpx;
}
.mr-32 {
  margin-right: 32rpx;
}
